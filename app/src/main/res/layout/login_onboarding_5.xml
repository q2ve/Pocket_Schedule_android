<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	>

	<TextView
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:text="Fifth Onboarding"
		android:textAlignment="center"
		android:textSize="56sp"/>

	<Button
		android:id="@+id/login_onboarding_authorize_button"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:text="login_onboarding_authorize_button"/>

	<Button
		android:id="@+id/login_onboarding_no_authorize_button"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:text="login_onboarding_no_authorize_button"/>

</LinearLayout>